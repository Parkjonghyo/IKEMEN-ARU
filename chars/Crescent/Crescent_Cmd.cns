[State 飛び道具]
Type = VarAdd
TriggerAll = !IsHelper
Trigger1 = Var(1)&8
var(1) = -8

[State 飛び道具射出可能]
Type = VarSet
TriggerAll = !IsHelper
TriggerAll = !(Var(1)&8)
Trigger1 = !NumHelper(1000)
Trigger1 = !NumHelper(2000)
Trigger2 = NumHelper(1000)
Trigger2 = !NumHelper(2000)
Trigger2 = Helper(1000),StateNo != 1005
Trigger3 = NumHelper(2000)
Trigger3 = !NumHelper(1000)
Trigger3 = Helper(2000),StateNo != 2005

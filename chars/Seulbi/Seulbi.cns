	Type=S
	Ctrl=0
	Anim=180
	velset=0,0
	sprpriority = 4

	[State 170,NotHit]
		Type=NotHitBy
		Trigger1=1
		value=SCA
		Time=1

	[State 181,Round not over]
		Type=assertSpecial
		Trigger1=Time <=90
		flag=Roundnotover



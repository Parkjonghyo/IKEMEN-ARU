trigger1   = ctrl || stateno = 500 || stateno = 52 || stateno = 5120 && (Anim = [5120,5127]) && AnimTime = 0
trigger2   = (stateno = [200,235]) || (stateno = [400,410]) || (stateno = [425,430])
trigger2   = movecontact > var(18)


Trigger2 = Command = "downback"
var(7) = 1

[State 再使用不能カウント開始]
Type = VarSet
TriggerAll = !IsHelper
TriggerAll = !Var(59)
TriggerAll = Var(7) = [1,10]
Trigger1 = Command != "holdback"

air.front = 12       ;공중에서 앞쪽 몸체범위
height = 60      ;캐릭터의 키. 이것이 크면 상대캐릭터가 점프로 뒤로 넘어갈수 없게 되
                 ;버립니다.
attack.dist = 160    ;공격을 했을 때 상대가 뒤 방향키를 눌렀을 때 가드포즈로 되게 하

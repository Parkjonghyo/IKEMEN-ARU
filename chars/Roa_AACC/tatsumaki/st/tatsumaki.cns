[Statedef 1111]     ;このステートを定義します。右の番号が「ステートナンバー」です。
type     = S        ;このステートの「状態」を指定。
                    ;S=立ち・C=しゃがみ・A=空中・L=横に倒れている・U=前のステートの状態を引き継ぐ
                    ;（地上にいても「A」なら、攻撃を食らうと空中へ吹っ飛ぶ）
movetype = I        ;このステートの「行動内容」を指定。
                    ;I=何もしてない・A=攻撃している・H=食らい状態・U=前のステートの行動内容を引き継ぐ
physics  = N        ;このステートの「実際の物理的な体勢」を指定。「小ネタ.txt」参照。
                    ;S=立ち・C=しゃがみ・A=空中・N=重力や摩擦を無視・U=前のステートの体勢を引き継ぐ
anim     = 1110    ;アニメを指定（アクションナンバー）。この場合airの[Begin Action 1000]を呼び出し使う。
velset   = 0,0      ;このステートを開始した瞬間の移動速度（Ｘ軸,Ｙ軸）。効果は「VelSet」を参照。
ctrl     = 0        ;このステート中にキーコントロールを可能にさせるかどうか。1でさせる。
poweradd = 0        ;パワーゲージの増減量。1以上で増え、-1以下で減る。
juggle   = 0        ;ジャグルポイントの消費量。ジャグルについては「小ネタ.txt」参照。
facep2   = 0        ;このステートを開始した瞬間に相手の方へ振り向くかどうか。1で向く。
hitdefpersist = 0   ;前のステートの「HitDef」の効果を持続させるかどうか。1でさせる。
movehitpersist = 0  ;前のステートからの「攻撃をヒットさせた状態」を持続させるかどうか。1でさせる。
hitcountpersist = 0 ;前のステートからの「HitCount」「UniqHitCount」の効果を持続させるかどうか。1でさせる。
sprpriority = 2    ;画像表示優先度。多ければ手前に、少なければ奥に優先される。


[Statedef 0]
type    = S
physics = S
sprpriority = 0

[State 0, 축 위치] ;지상 취소
type = varSet
trigger1 = time = 0
var(1) = 0

[State 0, 헬퍼라면 자동소멸]
type = DestroySelf
trigger1 = ishelper

[State 0, アニメ変更] ;뒤돌아 동작에서 이행
type = ChangeAnim
trigger1 = Anim != 0 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0 ;Turn anim over
value = 0

[State 0, 축 위치]; 지상 취소
type = PosSet
trigger1 = time = 0
y = 0

[State 0, 이동 속도]; Y 방향의 속도를 0으로
type = VelSet
trigger1 = time = 0
y = 0

[State 0, 이동 속도]; 느려질 4 프레임 경과 중지
type = VelSet
trigger1 = abs (vel x) <1
x = floor (vel x / 2)

[State 0, 상태 변경]; 생존 판정
type = ChangeState
trigger1 =! alive
value = 5050; 생존하지 않는 경우 스테이트 5050에

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

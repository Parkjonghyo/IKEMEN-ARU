;---------------------------------------------------------------------------
; HITA_FALL (knocked up, falling)
[Statedef 5050]
type    = A
movetype= H
physics = N

[State 5050, 1] ;Change anim when done with transition
type = ChangeAnim
trigger1 = AnimTime = 0
trigger1 = Anim = 25035
trigger2 = Time = 0     ;If no transition anim
trigger2 = Anim != 25035
trigger2 = (Anim != [25051, 25059]) && (Anim != [25061, 25069])
trigger2 = Anim != 25090 ;Not if hit off ground anim
value = 25050

[State 5050, 2] ;Coming down anim
type = ChangeAnim
trigger1 = anim = [25050,25059]
trigger1 = Vel Y >= ifelse(anim = 25050, 1, -2)
value = anim+10
persistent = 0

[State 5050, 3] ;Gravity
type = VelAdd
trigger1 = 1
y = GetHitVar(yaccel)

[State 5050, 4] ;Recover near ground
type = SelfState
TriggerAll = CanRecover
triggerAll = Vel Y > 0
triggerAll = Pos Y >= -20
TriggerAll = Alive
Trigger1 = Var(55) = 0
Trigger1 = command = "recovery"
Trigger2 = Var(55) > 0
Trigger2 = Random <= var(55)*100
Trigger3 = Helper(4444+ID),Var(6)
value = 5200 ;HITFALL_RECOVER

[State 5050, 5]; Recover in mid air
type = ChangeState
triggerall = Vel Y > -1
triggerall = alive
triggerall = CanRecover
Trigger1 = Var(55) = 0
Trigger1 = command = "recovery"
Trigger2 = Var(55) > 0
Trigger2 = Random <= var(55)*100
Trigger3 = Helper(4444+ID),Var(6)
value = 5210 ;HITFALL_AIRRECOVER


[State 5050, 6]
type = ChangeState
trigger1 = Vel Y > 0
trigger1 = Pos Y >= ifelse((anim = [5051,5059]) || (anim = [5061,5069]), 0, 25)
value = 5100 ;HIT_BOUNCE


;┳─────────┳──────────────────━
;｜ステートエントリー｜第４部
;┻─────────┻──────────────────━
[Statedef -1] ;必須項目
;┳━━━━━━┳━━━━━━━━━━━━━━━━━━━━━━
;┃一撃必殺奥義┃
;┻━━━━━━┻━━━━━━━━━━━━━━━━━━━━━━
[State -1, 北斗百裂拳]
type       = ChangeState
value      = 3900
triggerall = NumHelper(10000)
triggerall = var(59) = 0 && (command = "北斗百裂拳") && (command != "北斗有情猛翔破（強）") && RoundState = 2 || var(21) = 3900
triggerall = statetype != A && var(7) = 7
trigger1   = ctrl || stateno = 500 || stateno = 52 || stateno = 5120 && (Anim = [5120,5127]) && AnimTime = 0
trigger2   = (stateno = [200,235]) || (stateno = [400,430])
trigger2   = movecontact > var(18) || var(25) && !Helper(10000),NumTarget
trigger3   = (stateno = [200,245]) || (stateno = [400,430]) || stateno = 1200 || stateno = 1600
trigger3   = Time < 3


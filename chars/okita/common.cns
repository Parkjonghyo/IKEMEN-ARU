;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;コモンステート（基本動作およびやられ動作に関するステート）
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

;■『コモンステート』について■
;コモンステートとは、全キャラ共通の基本動作とやられ動作のステートです。
;通常は特に変更しない限り、MUGEN本体のdataフォルダ内「common1.cns」を使用します。

;しかし、common1.cnsのプログラム優先度は最低に属しているため、
;キャラのステートファイルにcommon1.cnsと同じ番号のステートが存在する場合は
;処理が上書きされ、common1.cns側の同じ番号のステートは無効となります。

;そのため、基本動作以外のステートにコモンステート用のナンバーを与えた場合
;キャラの基本動作に不具合が生じるおそれがあります。
;（例外として、一部使用されていないステートも存在します。そのステートについては
;　使用しても特に問題ありません。例えば110番、115番のステートは空中ダッシュ等に
;　流用されることが多いです。）


;■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■

;MUGENがデフォルトで参照するcommon1.cns内のコモンステートの早見表です。
;common1.cnsに存在しないナンバーは、ユーザーが任意でキャラに追加出来るステート。

;例外として、[Statedef 180]（勝利ポーズ）はcommon1.cns内に含まれていませんが
;MUGENのシステムで予め処理されているので、コモンステートに含めます。

;■コモンステート一覧■

;   0 - 通常立ち（振り返る動作もシステムがここで処理）
;  10 - 立ち状態からしゃがむ動作
;  11 - しゃがみ
;  12 - しゃがみ状態から立ち上がる動作
;  20 - 前進＆後退

;  40 - ジャンプ開始
;  45 - 空中ジャンプ開始
;  50 - ジャンプ
;  51 - ジャンプ後の下降（使われていない）
;  52 - 着地
;  55 - ハイジャンプ

; 100 -走る（ダッシュ）
; 105 - バックステップ
; 106 - バックステップ後の着地
; 110 - 用途不明（使われていない）※多くの場合空中ダッシュに使用されます。
; 115 - 用途不明（使われていない）※多くの場合空中バックステップに使用されます。

; 120 - ガード開始
; 130 - ガード受付（立ち）
; 131 - ガード受付（しゃがみ）
; 132 - ガード受付（空中）
; 140 - ガード終了
; 150 - ガード硬直（立ち）
; 151 - ガード硬直解除後の後退（立ち）
; 152 - ガード硬直（しゃがみ）
; 153 - ガード硬直解除後の後退（しゃがみ）
; 154 - ガード硬直（空中）
; 155 - ガード硬直解除後の後退（空中）
; 160 - 急降下

; 170 - タイムオーバーで負けた時
; 175 - タイムオーバーで引き分けた時
;（180 - 勝利時演出）
; 190 - イントロ準備
; 191 - イントロ

;5000 - 立ちやられ
;5001 - 立ちやられノックバック
;5010 - しゃがみやられ
;5011 - しゃがみやられノックバック

;5020 - 空中やられ
;5030 - 吹っ飛び開始
;5035 - 吹っ飛び（ダウン状態か否かの判断もしている）
;5040 - 吹っ飛びから自動受け身（ダウン状態ではない時）
;5050 - 吹っ飛び下降
;5070 - 転倒・硬直中
;5071 - 転倒

;5080 - 横臥時のやられ・硬直
;5081 - 横臥時のやられ
;5100 - 吹っ飛び後地面に激突
;5101 - 激突後バウンド
;5110 - 横たわる
;5111 - 横たわる（手前向き）（オプション）
;5120 - 起き上がる
;5150 - 横たわる（敗北時）

;5200 - 低空時の空中受け身予約
;5201 - 低空時の空中受け身
;5210 - 空中受け身

;5500 - コンティニュー画面時
;5900 - ラウンド開始前初期化専用ステート

;※この早見表とコモンステートについての解説は、
;　ちょっとこ丸氏の和訳KFM同梱のテキストにLycorisが補足、注釈を加えたものです。

;■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■


;　　　　　　　　　　　　　↓ここからステート記述↓　　　　　　　　　　　　　　


;※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※
;　　　　　　　　　　　　　　　！！！注意！！！　　　　　　　　　　　　　　　　
;以下のコモンステートにはMUGENデフォルトの「common1.cns」とは異なる内容の
;ステートが多々あります。決してデフォルトに上書きしたりしないでください。
;※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※※

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	立ち状態（待機）
;	・すべての動作の基点。基本的に動作を終えたらこのステートに戻ってきます。
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

[Statedef 0]
type    = S
physics = S
sprpriority = 0

[State 0, アニメ変更] ;振り向き動作から移行
type = ChangeAnim
trigger1 = Anim != 0 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0 ;Turn anim over
value = 0

[State 0, 軸位置] ;地上に戻す
type = PosSet
trigger1 = time = 0
y = 0

[State 0, 移動速度] ;Y方向の速度を0に
type = VelSet
trigger1 = time = 0
y = 0

[State 0, 移動速度] ;低速になるか4フレーム経過で停止
type = VelSet
trigger1 = abs(vel x) < 2
trigger2 = time = 4
x = 0

[State 0, ステート変更] ;生存判定
type = ChangeState
trigger1 = !alive
value = 5050 ;生存していない場合ステート5050へ

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	立ちからしゃがみへ移行
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

[Statedef 10]
type    = C
physics = C
anim = 10

[State 10, 速度乗算]
type = VelMul
trigger1 = time = 0
x = .75

[State 10, ステート変更] ;しゃがみステートに移行
type = ChangeState
trigger1 = animtime = 0
value = 11

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	しゃがみ状態
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

[Statedef 11]
type    = C
physics = C
anim = 11
sprpriority = 0

[State 11, アニメ変更] ;振り向き動作から移行
type = ChangeAnim
trigger1 = Anim = 6 && AnimTime = 0
value = 11

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	しゃがみから立ちへ移行
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

[Statedef 12]
type    = S
physics = S
anim = 12

[State 12, ステート変更] ;立ち状態へ移行
type = ChangeState
trigger1 = animtime = 0
value = 0

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	前進・後退
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

[Statedef 20]
type    = S
physics = S
sprpriority = 0

[State 20, 移動速度] ;前進する速度
type = VelSet
trigger1 = command = "holdfwd"
x = const(velocity.walk.fwd.x) ;定数で設定した速度

[State 20, 移動速度] ;後退する速度
type = VelSet
trigger1 = command = "holdback"
x = const(velocity.walk.back.x) ;定数で設定した速度

[State 20, アニメ変更] ;前進するアニメに変更
type = ChangeAnim
triggerall = vel x > 0
trigger1 = Anim != 20 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0
value = 20

[State 20, アニメ変更] ;後退するアニメに変更
type = ChangeAnim
triggerall = vel x < 0
trigger1 = Anim != 21 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0
value = 21

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	ジャンプ開始
;	・ジャンプするための準備段階です。この時点ではまだ地上にいます。
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

[Statedef 40]
type    = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1

[State 40, 無敵]
type = NotHitBy
trigger1 = 1
value = ,AT

;--------------------↓ジャンプする方向を制御する変数

[State 40, 変数を設定] ;ジャンプ開始時に変数「sysvar(1)」を0にセット
type = VarSet
trigger1 = time = 0
sysvar(1) = 0

[State 40, 変数を設定] ;コマンド"holdfwd"の場合「sysvar(1)」を1にセット
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 40, 変数を設定] ;コマンド"holdback"の場合「sysvar(1)」を-1にセット
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

;--------------------↑ジャンプする方向を制御する変数

[State 40, 移動速度] ;ジャンプ開始時の速度制御
type = VelSet
trigger1 = animtime = 0
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x), const(velocity.jump.back.x)))
y = const(velocity.jump.y)
;↑の変数からジャンプする方向を判別し、定数の[Velocity]より方向ごとの数値を引用。
;ジャンプしている間は常にこの数値が適用されます。

[State 40, 移動速度] ;ダッシュジャンプ時の速度制御
type = VelSet
trigger1 = animtime = 0
trigger1 = prevstateno = 100 ;ダッシュ
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)

[State 40, ステート変更] ;ジャンプ中ステートへ移行
type = ChangeState
trigger1 = animtime = 0
value = 50
ctrl = 1

;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	空中ジャンプ開始
;	・諸々の数値だけ設定して0Fでジャンプ中に移行します。
;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
[Statedef 45]
type    = A
physics = N
ctrl = 0
velset = 0,0

[State 45, アニメ変更]
type = ChangeAnim
trigger1 = SelfAnimExist(44)
value = 44

[State 45, アニメ変更]
type = ChangeAnim
trigger1 = !SelfAnimExist(44)
value = 41

[State 45, 変数を設定]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 45, 変数を設定]
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 45, 変数を設定]
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

[State 45, 移動速度]
type = VelSet
trigger1 = Time = 0
x = ifelse(sysvar(1)=0, const(velocity.airjump.neu.x), ifelse(sysvar(1)=1, const(velocity.airjump.fwd.x), const(velocity.airjump.back.x)))
y = const(velocity.airjump.y)

[State 45, ステート変更]
type = ChangeState
trigger1 = Time = 0
value = 50
ctrl = 1


;━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
;	ジャンプ中

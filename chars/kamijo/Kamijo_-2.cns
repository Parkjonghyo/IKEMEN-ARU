V        = 9
Value    = 0
Ignorehitpause = 1

[State -2,ライフ反?]
Type     = VarSet
Trigger1 = MoveType != H
V        = 9
Value    = -Life
Ignorehitpause = 1

[State -2,一打目?メ?ジ軽減]
Type     = LifeAdd
Trigger1 = MoveType = H
Trigger1 = Life && Alive
Trigger1 = Life < Var(9)
Value = Floor((Var(9)-Life)*FVar(9))
Ignorehitpause = 1
Absolute = 1

[State -2,?メ?ジ軽減]
Type     = DefenceMulSet
Trigger1 = Var(9) >= -LifeMax
Value    = 1-FVar(9)
Ignorehitpause = 1

[State -2,?ウン有効]
Type     = VarSet
Trigger1 = StateType = L || StateNo = 5070
Trigger1 = Var(9) = 0
V        = 9
Value    = -1-LifeMax
Ignorehitpause = 1

[State -2,?ウン中?メ?ジ軽減]
Type     = DefenceMulSet
Trigger1 = Var(9) < -LifeMax
Value    = (1-FVar(9))*Const(Data.Fall.Defence_Mul)
Ignorehitpause = 1

[State -2,一打終了]
Type     = VarSet
Trigger1 = MoveType = H
Trigger1 = Life <= Var(9) && Var(9)
V        = 9
Value    = 0
Ignorehitpause = 1

[State -2,ライフ]
Type     = VarSet
Trigger1 = MoveType = H
Trigger1 = Var(9) < 0 && Var(9) > -1-LifeMax
V        = 9
Value    = -Var(9)
Ignorehitpause = 1

;Valueが?メ?ジ軽減率です。Value = 0.5なら、?メ?ジが50%軽減されます。
[State -2,根性値];BadLuck,UnLucky,HardLuck(黒)
Type     = VarSet
Trigger1 = Var(3) = 0 || Var(3) = 1 || Var(3) = 4
FV       = 9
Value    = ifelse((Life > 0.3*LifeMax), 0.0, 0.3)
Ignorehitpause = 1

[State -2,根性値];HardLuck(白),HardLuck(赤)
Type     = VarSet
Trigger1 = Var(3) = 2 ||  Var(3) = 3
FV       = 9
Value    = ifelse((Life > 0.3*LifeMax), 0.0, 0.4)
Ignorehitpause = 1

[State -2,根性値];HardLuck(強),Calamity
Type     = VarSet
Trigger1 =  Var(3) = 5||Var(3) = 6
FV       = 9
Value    = ifelse((Life > 0.3*LifeMax), 0.0, 0.6)
Ignorehitpause = 1

[State -2,根性値];system
